                                    <xsd:documentation>Equals</xsd:documentation>
                                </xsd:annotation>
                            </xsd:enumeration>
                            <xsd:enumeration value="contains">
                                <xsd:annotation>
                                    <xsd:documentation>Contains</xsd:documentation>
